<meta charset="UTF-8">
<meta name="Pragma" content="no-cache">
<meta name="Expires" content="0">
<META HTTP-EQUIV="PRAGMA" CONTENT="NO-CACHE">
<script>
<%# IMPORTANT!!!! %>
<%# ALWAYS LOAD THIS *FIRST* %>
window.globalConfig = <%= raw(@global_config) %> // SEE COMMENTS!;
<%# READ COMMENT ABOVE %>
window.process = {
  env: {
    NODE_ENV: <%= raw(Rails.env.inspect) %>
  }
}
</script>
<%= render "addons" %>
<meta content="width=device-width, initial-scale=1" name="viewport">
<%= stylesheet_link_tag 'application' %>
<link href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,700,700i" rel="stylesheet">
<%# Rails.env.production? ? javascript_include_tag(*webpack_asset_paths("new_commons")) : "" %>
<% if false %>
  <script async src="http://<%= ENV["API_HOST"] %>:8080/target/target-script-min.js#anonymous">
  </script>
<% end %>
